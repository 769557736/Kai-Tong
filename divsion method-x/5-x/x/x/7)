package b;
import java.util.Scanner;
public class b25 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("请输入数字：");
		int num=scan.nextInt();
		if(num%5==0&&num%7==0){
			System.out.println("yes");
		}else{
			System.out.println("no");
		}

	}

}
