package b;
import java.util.Scanner;
public class b5 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
	System.out.print("请输入四位数：");
	int num=scan.nextInt();
	int x,x1,x2,x3;
	x=num/1000;
	num=num%1000;
	x1=num/100;
	num=num%100;
	x2=num/10;
	num=num%10;
	x3=num/1;
	num=num%1;
	
	System.out.print("输出结果为：");
    System.out.print(x3*1000+x2*100+x1*10+x);
	}

}
